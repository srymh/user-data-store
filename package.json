{
  "name": "user-data-store",
  "description": "",
  "version": "0.1.5-alpha.1",
  "author": "srymh",
  "files": [
    "/dist-cjs",
    "/dist-esm"
  ],
  "keywords": [],
  "license": "MIT",
  "main": "dist-cjs/index.js",
  "module": "dist-esm/index.js",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "tsc --project . --module esnext --outDir ./dist-esm",
    "prebuild": "rimraf ./dist-*",
    "prepare": "npm run build",
    "test": "jest --watch",
    "watch:cjs": "npm run build:cjs -- --watch --noEmit",
    "watch:esm": "npm run build:esm -- --watch --noEmit"
  },
  "types": "dist-esm/index.d.ts",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/md5": "^2.2.1",
    "electron": "^10.1.5",
    "electron-store": "5.2.0",
    "jest": "^26.6.3",
    "localforage": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "md5": "^2.3.0"
  }
}
